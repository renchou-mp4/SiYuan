e\u003cT\u003e(ResourceCollectionData collector, string category, params string[] extensions) where T : Object\n    {\n        string[] guids = AssetDatabase.FindAssets($\"t:{typeof(T).Name}\");\n        \n        foreach (string guid in guids)\n        {\n            string path = AssetDatabase.GUIDToAssetPath(guid);\n            \n            // è¿‡æ»¤æ–‡ä»¶ç±»å‹\n            if (!IsValidExtension(path, extensions)) continue;\n            \n            var resourceInfo = new ResourceInfo\n            {\n                Guid = guid,\n                Category = category,\n                Name = Path.GetFileNameWithoutExtension(path),\n                FullPath = path,\n                RelativePath = GetRelativePath(path),\n                FileSize = new FileInfo(path).Length,\n                Type = typeof(T).Name\n            };\n            \n            collector.Add(resourceInfo);\n        }\n    }\n}\n\n[System.Serializable]\npublic class ResourceCollectionData\n{\n    public List\u003cResourceInfo\u003e Resources = new List\u003cResourceInfo\u003e();\n    \n    public void Add(ResourceInfo info) =\u003e Resources.Add(info);\n    public int Count =\u003e Resources.Count;\n}\n\n[System.Serializable]\npublic class ResourceInfo\n{\n    public string Guid;\n    public string Category;\n    public string Name;\n    public string FullPath;\n    public string RelativePath;\n    public long FileSize;\n    public string Type;\n}\n"},{"Type":"NodeCodeBlockFenceCloseMarker","Data":"```"}]},{"ID":"20251118021330-hp9re33","Type":"NodeHeading","HeadingLevel":3,"Properties":{"id":"20251118021330-hp9re33","updated":"20251118021330"},"Children":[{"Type":"NodeText","Data":"é˜¶æ®µ2ï¼šExcel/JSONæ–‡ä»¶ç”Ÿæˆ"}]},{"ID":"20251118021330-sigrbuu","Type":"NodeCodeBlock","IsFencedCodeBlock":true,"Properties":{"id":"20251118021330-sigrbuu","updated":"20251118021330"},"Children":[{"Type":"NodeCodeBlockFenceOpenMarker","Data":"```"},{"Type":"NodeCodeBlockFenceInfoMarker","CodeBlockInfo":"Y3No